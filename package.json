{
  "name": "onf-chat",
  "version": "0.1.0",
  "description": "Simple chat server",
  "scripts": {
    "build": "mkdir -p bin && cmake CMakeLists.txt",
    "chat": "./bin/simple_chat",
    "clean": "rm -rf CMakeFiles CMakeCache.txt cmake_install.cmake bin",
    "prepublish": "git submodule init && npm run update && npm run build",
    "update": "git submodule update --remote"
  },
  "keywords": [
    "chat",
    "telnet"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/btoll/onf-simple-chat"
  },
  "author": "Benjamin Toll <benjam72@yahoo.com> (http://www.benjamintoll.com)",
  "license": "GPLv3"
}

